{
  "name": "@convex-dev/convex-kinde-auth",
  "version": "0.0.4",
  "description": "Convex database adapter for Kinde Auth",
  "homepage": "https://convex.dev/",
  "repository": "https://github.com/get-convex/convex-kinde-auth",
  "keywords": [
    "auth",
    "kinde",
    "clerk",
    "authentication",
    "db",
    "database",
    "react"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "files": [
    "src",
    "cjs",
    "esm",
    "README.md"
  ],
  "exports": {
    "./react": {
      "types": "./src/react.tsx",
      "import": "./esm/react.js",
      "require": "./cjs/react.js"
    }
  },
  "typesVersions": {
    "*": {
      "react": [
        "./src/react.tsx"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf cjs esm && tsc -p .tsconfig.cjs.json && tsc -p .tsconfig.esm.json && echo '{\"type\": \"commonjs\"}' > cjs/package.json"
  },
  "peerDependencies": {
    "convex": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@kinde-oss/kinde-auth-react": "^3.0.28"
  },
  "devDependencies": {
    "@kinde-oss/kinde-auth-react": "^3.0.28",
    "@types/node": "^20.4.6",
    "@types/react-dom": "^18.2.7",
    "@types/react": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint": "^8.45.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
